<p id="notice"><%= notice %></p>

<% if current_user %>
<%= current_user.profile.id %>
<%= link_to 'View my profile', profile_path(current_user.profile.id) %> 
<% end %>

<h1>RoadTripper</h1>

<table>
  <thead>
    <tr>
      <th>RoadTripper</th>

    </tr>
  </thead>

  <tbody>
    <% @road_trips.each do |road_trip| %>
      <tr>
        <td><%= road_trip.title %></td>
        <td><%= link_to 'View Trip profile', road_trip %></td>
        <td><%= link_to 'Destroy', road_trip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Road trip', new_road_trip_path %>   